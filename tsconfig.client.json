{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "sourceMap": true,
    "strict": true,
    "outDir": ".",
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "watch": true,
    "declaration": true
  },
  "lib": ["es5", "es2015.promise", "dom", "es2015"],
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/client.ts"
  ]
}
